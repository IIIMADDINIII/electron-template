{
  "name": "@app/common",
  "version": "0.1.0",
  "description": "Common Files between Main, Preload and Renderer",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/esm/index.d.ts",
        "default": "./dist/esm/index.js"
      },
      "require": {
        "types": "./dist/cjs/index.d.ts",
        "default": "./dist/cjs/index.js"
      }
    },
    "./comlink": {
      "import": {
        "types": "./dist/esm/comlink.d.ts",
        "default": "./dist/esm/comlink.js"
      },
      "require": {
        "types": "./dist/cjs/comlink.d.ts",
        "default": "./dist/cjs/comlink.js"
      }
    }
  },
  "scripts": {
    "build": "pnpm run gulp build",
    "gulp": "pnpm --package=@iiimaddiniii/js-build-tool@0.3.2 dlx js-build-tool"
  },
  "author": "IIIMADDINIII",
  "license": "MIT",
  "devDependencies": {
    "rollup": "^3.19.1",
    "typescript": "^5.0.2",
    "@rollup/plugin-typescript": "^11.0.0",
    "@rollup/plugin-commonjs": "^24.0.1",
    "@rollup/plugin-node-resolve": "^15.0.1",
    "@rollup/plugin-terser": "^0.4.0",
    "rollup-plugin-include-sourcemaps": "^0.7.0",
    "@rollup/pluginutils": "^5.0.2",
    "rollup-config-iiimaddiniii": "^0.1.0",
    "rollup-plugin-consts": "^1.1.0"
  },
  "dependencies": {
    "tslib": "^2.5.0"
  }
}